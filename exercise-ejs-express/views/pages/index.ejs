<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
    <h1>
        Loving our pets
    </h1>
</head>
<body class="container">

    <header>
        <%- include('../partials/header'); %>
        <p>header</p>
    </header>

    <main>
        <div class="jumbotron">
            <h1>Top Mascots</h1>
            <p><%= tagline %></p>
            <ul>
                <% mascots.forEach(function(mascot) { %>
                    <li>
                        <strong><%= mascot.name %></strong>
                        representing <%= mascot.organization %>, born <%= mascot.birth_year %>
                    </li>
                <% }); %>
            </ul>
        </div>
    </main>

    <footer>
        <%- include('../partials/footer'); %>
    </footer>

</body>
</html>